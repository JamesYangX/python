# 첫번째 방법

sum = 0 
res = ""

for i in range(1, 100):
    if i % 2 == 1 :
        if i == 99 :
            res += str(i)
        else :
            res += str(i) + " + "
        sum += i
    
print(res + " = " + str(sum))

# 두번째 방법

sum = 0
res = []
for i in range(1, 100, 2):
    res.append(str(i))
    sum += i
print(" + ".join(res) +  " = " + str(sum))
